<div>
    <form id="signup">
        <h2>Email</h2>
        <input type="text" id="signup-email" name="Email">
        <h2>Password</h2>
        <input type="text" id="signup-password" name="Password">
        <h2>First Name</h2>
        <input type="text" id="first-name" name="First Name">
        <h2>Last Name</h2>
        <input type="text" id="last-name" name="Last Name">
        <h2>Age</h2>
        <select id="age" name="Age">
            <option value="" disabled selected>Select Your Age</option>
        </select>
        <h2>Gender</h2>
        <select id="gender" name="Gender">
            <option value="" disabled selected>Select Your Gender</option>
            <option value="Male">Male</option>
            <option value="Female">Female</option>
        </select>
        <h2>Previous Marriage</h2>
        <select id="marriage" name="Marriage">
            <option value="" disabled selected>Select</option>
            <option value="No">No</option>
            <option value="One">One</option>
            <option value="Multiple">Multiple</option>
        </select>
        <h2>Children</h2>
        <select id="children" name="Children">
            <option value="" disabled selected>Select</option>
            <option value="None">None</option>
            <option value="1-2">1-2</option>
            <option value="3 or more">3 or more</option>
        </select>
        <h2>Family Drama</h2>
        <select id="family" name="Family">
            <option value="" disabled selected>Select</option>
            <option value="Yes">Yes</option>
            <option value="No">No</option>
        </select>
        <h2>Smoker</h2>
        <select id="smoker" name="Smoker">
            <option value="" disabled selected>Select</option>
            <option value="Yes">Yes</option>
            <option value="No">No</option>
        </select>
        <h2>Debt</h2>
        <select id="debt" name="Debt">
            <option value="" disabled selected>Select</option>
            <option value="Yes">Yes</option>
            <option value="No">No</option>
        </select>
        <h2>Travel for Work</h2>
        <select id="travel" name="Travel">
            <option value="" disabled selected>Select</option>
            <option value="100%">100%</option>
            <option value="50-100%">50-100%</option>
            <option value="0-50%">0-50%</option>
        </select>
        <h2>Bio</h2>
        <input type="text" id="bio" name="Bio">
    </form>
    <button type="button" id="signupBtn" class="btn btn-lg">Submit</button>
</div>

<script src='./js/aboutyou.js'></script>